{
  "name": "n8n-nodes-advanced-http",
  "version": "0.1.0",
  "description": "Advanced HTTP Request node for n8n with dynamic type conversion and enhanced security",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "http",
    "request",
    "api",
    "type-conversion"
  ],
  "author": {
    "name": "dwillitzer",
    "email": "dwillitzer@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwillitzer/n8n-nodes-advanced-http.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp package.json dist/ && cp README.md dist/ && cp LICENSE dist/",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/AdvancedHttp/AdvancedHttp.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "n8n-core": "^1.9.0",
    "n8n-workflow": "^1.9.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "license": "MIT"
}
